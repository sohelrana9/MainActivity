<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FF9800"
    tools:context=".Main6Activity">

    <Button
        android:id="@+id/button5"
        android:layout_width="405dp"
        android:layout_height="130dp"
        android:layout_marginTop="84dp"
        android:background="#E91E63"
        android:text="Baby bathtub"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.007" />

    <Button
        android:id="@+id/button9"
        android:layout_width="405dp"
        android:layout_height="130dp"
        android:layout_marginTop="84dp"
        android:background="#E91E63"
        android:text="Baby lotion"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.434" />

    <Button
        android:id="@+id/button17"
        android:layout_width="405dp"
        android:layout_height="130dp"
        android:layout_marginTop="84dp"
        android:background="#E91E63"
        android:text="Soft washcloths"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.933" />

    <EditText
        android:id="@+id/editText9"
        android:layout_width="228dp"
        android:layout_height="42dp"
        android:layout_marginTop="4dp"
        android:background="#660DF005"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Baby's Products List"
        android:textColor="#F70505"
        android:textColorHighlight="#F30303"
        android:textColorHint="#FA0505"
        android:textColorLink="#F80303"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>